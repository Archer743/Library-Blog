@using Library_Web_App.Data.Entities
@model List<User>

@if (User.Identity?.IsAuthenticated ?? false && User.IsInRole("ADMIN"))
{
	@if (Model.Count == 0)
	{
			<p>No Users Found!</p>
	}
	else
	{
			<h3>Users</h3>

		@foreach (User user in Model)
		{
					<div>
						<h6>- @user.UserName</h6>
					</div>
		}
	}
}
